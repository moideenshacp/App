            <section class="content-main">
                <div class="row">
                    <div class="col-9">
                        <div class="content-header">
                            <h2 class="content-title">Update Coupon</h2>
                         
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <div class="card mb-4">
                            <div class="card-header">
                                <h4>Coupon</h4>
                            </div>
                            <div class="card-body">
                                <form method="post">
                                    <% if(typeof messages!=='undefined' ){ %>

                                        <p style="color: red;text-align: center;" class="message">
                                            <%= messages %>
                                        </p>
                                        <% } %>
                                        <% if(typeof message!=='undefined' ){ %>

                                            <p style="color: green;text-align: center;" class="message">
                                                <%= message %>
                                            </p>
                                            <% } %>
                                    <div class="mb-4">
                                        <label  for="product_name" class="form-label">Coupon Name</label>
                                        <input type="text" name="name" placeholder="Type here" class="form-control" value="<%=couponFind.name%>"  required  />
                                    </div>
                                    
                                    <div class="row">
                                        <div class="col-lg-4">
                                            <div class="mb-4">
                                                <label class="form-label">Coupon Code</label>
                                                <div class="row gx-2">
                                                    <input name="code" type="text" placeholder="Type here" class="form-control" value="<%=couponFind.code%>" required  />
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-lg-4">
                                            <div class="mb-4">
                                                <label class="form-label">Min Amount</label>
                                                <input name="minamount" type="text" placeholder="Type here " class="form-control" value="<%=couponFind.minAmount%>" required />
                                            </div>
                                        </div>
                                        <div class="col-lg-4">
                                            <div class="mb-4">
                                                <label class="form-label">Discount</label>
                                                <input name="discount" type="text" placeholder="Type here %" class="form-control" value="<%=couponFind.discount%>" required />
                                            </div>
                                        </div>
                                        <div class="col-lg-4">
                                            <div class="mb-4">
                                                <label class="form-label">Expiry Date</label>
                                                <input name="expireDate" type="date" class="form-control" value="<%=formattedExpireDate%>" required />
                                            </div>
                                        </div>
                                        
                                        <div class="mb-4">
                                            <label class="form-label">Full description</label>
                                            <textarea name="description" placeholder="Type here" class="form-control" rows="4" required><%=couponFind.description%></textarea>
                                        </div>
                                        <input class="input" type="hidden" name="id" value="<%= couponFind._id %>">


                                    

                                    
                                    </div>
                                    <div class="col-lg-9">
                                            
                                      
                                   
                                    <div>
                                        <!-- <button class="btn btn-light rounded font-sm mr-5 text-body hover-up">Save to draft</button> -->
                                        <button type="submit" class="btn btn-md rounded font-sm hover-up">Upcate Coupon</button>
                                    </div>
                                </form>
                            </div>
                        </div>
                        <!-- card end// -->
                    
                        <!-- card end// -->
                    </div>
              
