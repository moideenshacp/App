            <section class="content-main">
                <div class="content-header">
                    <div>
                        <h2 class="content-title card-title">Coupons List</h2>
                       
                    </div>
                    <div>
           
                        <a href="/admin/Addcoupon" class="btn btn-primary btn-sm rounded">Create new</a>
                    </div>
                </div>
                <div class="card mb-4">
                    <header class="card-header">
                        <div class="row align-items-center">
                            <div class="col col-check flex-grow-0">
                                <div class="form-check ms-2">
                                    <input class="form-check-input" type="checkbox" value="" />
                                </div>
                            </div>
                            <div class="col-md-3 col-12 me-auto mb-md-0 mb-3">
                                <select class="form-select">
                                    <option selected>All category</option>
                                    <option>Electronics</option>
                                    <option>Clothes</option>
                                    <option>Automobile</option>
                                </select>
                            </div>
                            <div class="col-md-2 col-6">
                                <input type="date" value="02.05.2021" class="form-control" />
                            </div>
                            <div class="col-md-2 col-6">
                                <select class="form-select">
                                    <option selected>Status</option>
                                    <option>Active</option>
                                    <option>Disabled</option>
                                    <option>Show all</option>
                                </select>
                            </div>
                        </div>
                    </header>
                    <!-- card-header end// -->
                    <div class="card-body">
                        <div class="table-responsive">
                            <table class="table table-hover">
                                <thead>
                                    <tr>
                                        <th>ID</th>
                                        <th>Name</th>
                                        <th>Code</th>
                                        <th>MinAmout</th>
                                        <th>Discout</th>

                                        <th scope="col">Status</th>

                                        <th class="text-end">Action</th>
                                    </tr>
                                </thead>
                                <%coupons.forEach((coupon,index)=>{%>

                                <tbody>
                                   
    
                                        <tr>
                                            <td><%=index+1%></td>
                                            <td width="40%">
                                                <a href="#" class="itemside" data-src="">
                                                    
                                                    <div class="info pl-3">
                                                        <h6 class="mb-0 title">
                                                            <%=coupon.name%>
                                                        </h6>
                                                     
                                                    </div>
                                                </a>
                                            </td>
                                            <td>
                                                <%=coupon.code%>
                                            </td>
                                            <td>
                                                <%=coupon.minAmount%>
                                            </td>
                                            <td><%=coupon.discount%></td>

                                            <td><span class="badge rounded-pill alert-warning">
                                                <%=coupon.status%>
                                            </span></td>
                                            <td class="text-end">
                                                <button class="btn btn-md rounded font-sm" style="align-items: center; padding: 0;">
                                                    <a href="/admin/couponEdit?id=<%=coupon._id%>" style="display: block; padding: 0.5rem; text-decoration: none; color: inherit;">Edit</a>
                                                </button>
                                               <% if(coupon.status==false){%>
                                                <button class="btn btn-md rounded font-sm" style="align-items: center; padding: 0;">
                                                    <a href="/admin/couponList?id=<%=coupon._id%>" style="display: block; padding: 0.5rem; text-decoration: none; color: inherit;">List</a>
                                                </button>
                                               <%}else{%>
                                                <button class="btn btn-md rounded font-sm" style="align-items: center; padding: 0;">
                                                    <a href="/admin/couponList?id=<%=coupon._id%>" style="display: block; padding: 0.5rem; text-decoration: none; color: inherit;">UnList</a>
                                                </button>
                                               <%}%>
   
                                            </td>
                                           
                                        </tr>
                                </tbody>
                                <%})%>

                            </table>
                            <!-- table-responsive.// -->
                        </div>
                    </div>
