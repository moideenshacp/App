                                            <th>Product</th>
                                            <th>Price</th>
                                            <th>Quantity</th>
                                            <th>Total</th>
                                            <th></th>
                                        </tr>
                                    </thead>
                                    <% cartPoducts.forEach(cart=> { %>
                                        <% cart.products.forEach(cartProduct=> { %>
                                            <tbody>
                                                <tr>
                                                    <td class="product-col">
                                                        <div class="product">
                                                            <figure class="product-media">
                                                                <a href="/productDetail?id=<%=cartProduct.product._id%>">
                                                                    <img src="/productImages/productImages/<%=cartProduct.product.image[0]%>"
                                                                        alt="Product image">
                                                                </a>
                                                            </figure>
                                                            <h3 class="product-title">
                                                                <a href="/productDetail?id=<%=cartProduct.product._id%>">
                                                                    <%= cartProduct.product.name %>
                                                                </a>
                                                            </h3><!-- End .product-title -->
                                                        </div><!-- End .product -->
                                                    </td>
                                                    <%if(cartProduct.product.offerprice>0 && cartProduct.product.category.offerprice > 0){%>
                                                        <td class="price-col">₹<%=Math.round(cartProduct.product.offerprice - (cartProduct.product.offerprice * cartProduct.product.category.offerprice) / 100)%></td>

                                                    <%}else if(cartProduct.product.offerprice>0){%>
                                                        <td class="price-col">₹<%=cartProduct.product.offerprice%></td>

                                                    <%}else if(cartProduct.product.category.offerprice > 0){%>
                                                        <td class="price-col">₹<%=Math.round(cartProduct.product.price - (cartProduct.product.price * cartProduct.product.category.offerprice) / 100)%></td>

                                                    <%}else{%>
                                                        <td class="price-col">₹<%=cartProduct.product.price%></td>
                                                    <%}%>



                                                    
                                                    <td class="quantity-col">
                                                        <div class="cart-product-quantity">
                                                            
                                                            
                                                            <!-- <input type="number" class="form-control"
                                                                value="<%= cartProduct.quantity %>" min="1"
                                                                max="<%=cartProduct.product.quantity%>" id="qty"
                                                                step="1" data-decimals="0" required
                                                                onchange="updatePrice('<%=cartProduct.product._id%>',this.value)"> -->
                                                                <%
                                                                var maxQty = Math.min(cartProduct.product.quantity, 5);
                                                            %>

                                                            <%if(cartProduct.product.quantity<=0){%>
                                                                <button class=" btn-out-of-stock" disabled><span>Out of
                                                                    Stock</span></button>
                                                            <%}else{%>
                                                                <input type="number" id="qty" class="form-control" value="<%= cartProduct.quantity %>" min="1" max="<%= maxQty %>" step="1" data-decimals="0" required onchange="updatePrice('<%= cartProduct.product._id %>', this.value)">

                                                           <% }%>
                                                            
                                                        </div><!-- End .cart-product-quantity -->
                                                        
                                                    </td>
                                             
                                                    <%if(cartProduct.product.offerprice>0 && cartProduct.product.category.offerprice > 0){%>
                                                    <td class="total-col"
                                                        id="productttlPrice<%=cartProduct.product._id%>">₹<%=
                                                        Math.round(cartProduct.product.offerprice - (cartProduct.product.offerprice * cartProduct.product.category.offerprice) / 100) * cartProduct.quantity %>
                                                    </td>
                                                    <td class="remove-col"><button class="btn-remove"
                                                            onclick="removeProduct('<%=cartProduct.product._id%>')"><i
                                                                class="icon-close"></i></button></td>
                                                    <%}else if(cartProduct.product.offerprice>0){%>
                                                        <td class="total-col"
                                                        id="productttlPrice<%=cartProduct.product._id%>">₹<%=
                                                            cartProduct.product.offerprice * cartProduct.quantity %>
                                                    </td>
                                                    <td class="remove-col"><button class="btn-remove"
                                                            onclick="removeProduct('<%=cartProduct.product._id%>')"><i
                                                                class="icon-close"></i></button></td>
                                                
                                                <%}else if(cartProduct.product.category.offerprice > 0){%>
                                                    <td class="total-col"
                                                        id="productttlPrice<%=cartProduct.product._id%>">₹<%=
                                                        Math.round(cartProduct.product.price - (cartProduct.product.price * cartProduct.product.category.offerprice) / 100) * cartProduct.quantity  %>
                                                    </td>
                                                    <td class="remove-col"><button class="btn-remove"
                                                            onclick="removeProduct('<%=cartProduct.product._id%>')"><i
                                                                class="icon-close"></i></button></td>

                                                <%}else{%>
                                                    <td class="total-col"
                                                        id="productttlPrice<%=cartProduct.product._id%>">₹<%=
                                                        cartProduct.product.price * cartProduct.quantity %>
                                                    </td>
                                                    <td class="remove-col"><button class="btn-remove"
                                                            onclick="removeProduct('<%=cartProduct.product._id%>')"><i
                                                                class="icon-close"></i></button></td>

                                                <%}%>
                                            </tr>
                                            </tbody>
                                            <% }) %>
                                                <% }) %>

