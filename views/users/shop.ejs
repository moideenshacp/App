                                                            <label class="custom-control-label" for="cat-<%= item._id %>"><%= item.name %></label>
                                                        </div><!-- End .custom-checkbox -->
                                                    </div><!-- End .filter-item -->
                                                </div><!-- End .widget-body -->
                                            </div><!-- End .collapse -->
                                        </div><!-- End .widget -->
                                    <% } %>
                                <% }) %>
                                
                           <br><br>

